d1: READ FILE=plumed.in VALUES=c1
d2: READ FILE=plumed.in VALUES=c2
d3: READ FILE=plumed.in VALUES=c3

ff: COLLECT_FRAMES STRIDE=1 ARG1=d1 ARG2=d2 ARG3=d3
data: VSTACK ARG1=ff.d1 ARG2=ff.d2 ARG3=ff.d3
dataT: TRANSPOSE ARG=data
dists: DISSIMILARITIES SQUARED ARG1=data ARG2=dataT
lwe: CUSTOM ARG1=ff.logweights FUNC=exp(x) PERIODIC=NO

smap: SKETCHMAP ...
  DISSIMILARITIES=dists WEIGHTS=lwe NLOW_DIM=2 
  HIGH_DIM_FUNCTION={SMAP R_0=2 A=3 B=9} 
  LOW_DIM_FUNCTION={SMAP R_0=2 A=2 B=2} 
  USE_SMACOF SMACTOL=1E-2 
...

PRINT ARG=smap.*,lwe FILE=smap FMT=%8.1f

